{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"lib": [
			"ESNext.String",
			"ESNext.Array",
			"ESNext.Intl",
			"ESNext.Symbol",
			"DOM"
		],
		"target": "ESNext"
	}
}
